{
  "build": {
    "beforeBuildCommand": "elm make src/Main.elm --output=public/main.js",
    "beforeDevCommand": "elm make src/Main.elm --output=public/main.js --optimize",
    "devPath": "http://localhost:4000",
    "distDir": "../public"
  },
  "package": {
    "productName": "Counter",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "io.counter-tauri",
      "longDescription": "Elm counter app built with tauri.app",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 230,
        "resizable": true,
        "title": "Foo",
        "width": 195
      }
    ]
  }
}
